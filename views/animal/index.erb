<h2>All Animals</h2>

<section>
  <table>
    <thead>
      <tr>
        <td>Name</td>

        <td>Age</td>

        <td>admission date</td>

        <td>Status</td>

        <td>Breed</td>



      </tr>
    </thead>
    <tbody>
      <% @animals.each { |animal| %>
      <tr>
        <td> <a href="/animals/ <%= animal.id %>"> <%= animal.name %></a></td>

        <td><%= animal.age %></td>

        <td><%= animal.admission_date %></td>

        <td><%= animal.status %></td>

        <td><%= animal.get_breed_name_by_id  %></td>



        <td>


        </td>

      </tr>
      <% } %>
    </tbody>
  </table>
</section>
